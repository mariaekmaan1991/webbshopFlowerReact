,\n\n  ListProduct,\n}) {\n  function saveInfo2(e) {\n    console.log(e, \"hej\");\n    e.preventDefault();\n  }\n\n  let productList =\n    ListProduct &&\n    ListProduct.map((data, index = parseInt(data.id)) => {\n      console.log(data, \"hh\");\n      return (\n        <div className=\"Admin-Display-Product-Main-Container\">\n          <div className=\"Admin-Display-Product-Container\" key={index}>\n            <div className=\"Admin-Display-Product-Container-Name\">\n              {`id: ${data.id}`}\n              <h2 className=\"Admin-Display-Product-Name\">\n                {`Titel: ${data.name}`}\n              </h2>\n            </div>\n            <div className=\"Admin-Display-Product-Container-ImageUrl\">\n              <img\n                className=\"Admin-Display-Product-ImageUrl\"\n                src={data.imageUrl}\n                alt=\"\"\n              />\n            </div>\n            <div className=\"Admin-Display-Product-Container-Info\">\n              <h3> {`price: ${data.price}`}</h3>\n              <p>{`description: ${data.description}`}</p>\n            </div>\n            <div className=\"Admin-Display-Product-Container-Category\">\n              <p>{` category: ${data.category}`}</p>\n            </div>\n            <div className=\"Admin-Display-Product-Container-Category\">\n              <button\n                className=\"Admin-Display-Product-Container-Button\"\n                onClick={() => deleteProduct(data.id)}\n              >\n                radera\n              </button>\n              <button\n                className=\"Admin-Display-Product-Container-Button\"\n                onClick={() => setUpdateFormValues(data)}\n              >\n                upptatera\n              </button>\n            </div>\n          </div>\n        </div>\n      );\n    });\n\n  return <div className=\"Admin-Display-ProductList-Main\">{productList}</div>;\n}\n","/Users/maria/Examensarbete2020/webbshopflowerreactwithFirebase/src/co